<UserControl x:Class="CompMs.App.Msdial.View.MsResult.SummarizedSpectrumView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:CompMs.App.Msdial.ViewModel.MsResult"
             xmlns:chart="clr-namespace:CompMs.App.Msdial.View.Chart"
             d:DataContext="{d:DesignInstance Type={x:Type vm:SummarizedSpectrumViewModel}}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <chart:SpectrumView DataContext="{Binding SpectrumViewModel, Mode=OneTime}"/>
    </Grid>
</UserControl>
